variable "region" {
  description = "GCP region"
  type        = string
  default     = "us-east1"
}
variable "zone" {
  description = "GCP zone"
  type        = string
  default     = "us-east1-c"
}
variable "project_id" {
  description = "GCP project_id"
  type        = string
  default     = "[INSERT_PROJECT_ID]"
}
